<div class="container center">
  <%= form_for [:admin, @category] do |f| %>
    <%= render "shared/error_messages", object: f.object %>
    <%= f.text_field :title %>
    <%= f.label :title %>
    <br>
    <br>
    <%= f.submit "Stache that Category!", class: "btn light-green darken-4" %>
  <% end %>
</div>
