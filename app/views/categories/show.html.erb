<h3> <%= @category.title %> Mustaches: </h3><br>
<% @category.staches.each do |stache| %>
<div class="row">
      <div class="col s12 m6">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <span class="card-title">
              <%= link_to stache.name, stache_path(stache) %>
            </span>
            <p>Info about this stache.</p>
          </div>
          <div class="card-action">
            <a href="#">View This Stache</a>
          </div>
        </div>
      </div>
    </div>
<% end %>
